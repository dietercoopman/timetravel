{
  "name": "dietercoopman/timetravel",
  "description": "TimeTravel class",
  "type": "library",
  "authors": [
    {
      "name": "Dieter Coopman",
      "email": "dieter@deltasolutions.be"
    }
  ],
  "require": {
    "php": ">=8.0",
    "nesbot/carbon": "^2.44",
